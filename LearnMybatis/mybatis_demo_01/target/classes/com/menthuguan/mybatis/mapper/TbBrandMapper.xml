<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.menthuguan.mybatis.mapper.TbBrandMapper">
    <select id="selectBrandById" resultType="com.menthuguan.mybatis.pojo.Brand">
        select *
        from brand
        where id = ${brandId}
    </select>
</mapper>